<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<ScrollPane prefHeight="843.0" prefWidth="548.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1" fx:controller="swt.swl.topcard.controller.CreateRQCardController">
	<content>
		<VBox prefHeight="835.0" prefWidth="530.0" spacing="20.0">
			<children>
				<Label prefHeight="27.0" prefWidth="257.0" text="Create new Requirement Card: " underline="true" />
