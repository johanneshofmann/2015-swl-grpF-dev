				<HBox prefHeight="39.0" prefWidth="410.0" spacing="118.0">
					<children>
						<Label prefHeight="16.0" prefWidth="44.0" text="Title:" />
						<TextField fx:id="titleTextField" layoutX="138.0" layoutY="10.0" />
					</children>
				</HBox>
				<HBox fx:id="moduleHBox" prefHeight="39.0" prefWidth="358.0" spacing="40.0">
					<children>
						<Label prefHeight="27.0" prefWidth="121.0" text="Modulename:" />
